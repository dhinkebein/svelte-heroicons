{
  "name": "@dhinkebein/svelte-heroicons",
  "description": "Svelte wrappers for the Heroicons library.",
  "keywords": [
    "svelte",
    "svelte3",
    "sveltejs",
    "heroicons",
    "icons"
  ],
  "author": "Dan Hinkebein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhinkebein/svelte-heroicons/issues"
  },
  "homepage": "https://github.com/dhinkebein/svelte-heroicons",
  "repository": "github:dhinkebein/svelte-heroicons",
  "version": "0.1.7",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepackage": "node build",
    "package": "svelte-kit sync && svelte-package && publint",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],  
  "peerDependencies": {
    "@sveltejs/kit": "^2.46.5",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.46.5",
    "@sveltejs/package": "2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte": "^3.12.4",
    "heroicons": "^1.0.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.7.45",
    "svg-to-svelte": "^2.2.1",
    "publint": "^0.1.12",
    "tslib": "^2.3.1",
    "typescript": "^5.0.0"
  },
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
